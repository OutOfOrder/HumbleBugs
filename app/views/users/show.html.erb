<fieldset>
  <legend>User details</legend>
  <p>
    <b>Email:</b>
    <%= @user.email %>
  </p>

  <p>
    <b>Name:</b>
    <%= @user.name %>
  </p>

  <p>
    <b>Password digest:</b>
    <%= @user.password_digest %>
  </p>


  <%= link_to 'Edit', edit_user_path(@user) %> |
  <%= link_to 'Back', users_path %>
</fieldset>
