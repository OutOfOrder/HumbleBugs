<h1>Listing releases</h1>

<table>
  <tr>
    <th>Notes</th>
    <th>Owner</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @releases.each do |release| %>
  <tr>
    <td><%= release.notes %></td>
    <td><%= release.owner %></td>
    <td><%= release.url %></td>
    <td><%= link_to 'Show', release %></td>
    <td><%= link_to 'Edit', edit_release_path(release) %></td>
    <td><%= link_to 'Destroy', release, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Release', new_release_path %>
